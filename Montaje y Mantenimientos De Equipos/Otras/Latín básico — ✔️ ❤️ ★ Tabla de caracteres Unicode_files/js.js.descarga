$(document).ready(function(){$(".tab_content").hide();$("ul.tabs li:first").addClass("active").show();$(".tab_content:first").show();$("ul.tabs li").click(function(){$("ul.tabs li").removeClass("active");$(this).addClass("active");$(".tab_content").hide();var activeTab=$(this).find("a").attr("href");$(activeTab).fadeIn();return false;});var content_menu=$('#table-content-menu');if(content_menu.length > 0){content_menu.find('a').on('click',function (e){var id=$(this).attr('href');if(id !=''){var block=$(id);if(block.length > 0){e.preventDefault();$('html,body').animate({'scrollTop':block.offset().top},500);}}});}});