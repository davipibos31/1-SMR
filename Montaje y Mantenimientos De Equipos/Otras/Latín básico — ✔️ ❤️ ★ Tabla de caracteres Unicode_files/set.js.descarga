(function(){var textArea;function isOS(){return navigator.userAgent.toLowerCase().match(/ipad|iphone/i);}function createTextArea(text){textArea=document.createElement('textArea');textArea.setAttribute('readonly',true);textArea.style.position='fixed';textArea.value=text;document.body.appendChild(textArea);}function selectText(){var range,selection;if (isOS()){range=document.createRange();range.selectNodeContents(textArea);selection=window.getSelection();selection.removeAllRanges();selection.addRange(range);textArea.setSelectionRange(0,999999);}else{textArea.select();}}function copyToClipboard(){document.execCommand('copy');document.body.removeChild(textArea);}function copy(text){createTextArea(text);selectText();copyToClipboard();};$(document).ready(function(){$(document).delegate(".symbol-copy","click",(event)=>{let target=$(event.currentTarget);let symbol=target.data("symbol");if (typeof symbol==='undefined'){symbol=target.data("copy");}copy(symbol);$(".set-v2-popup-symbol").text(symbol);$(".set-v2-popup").fadeIn(200);setTimeout(function(){$(".set-v2-popup").fadeOut(200);},1000);});$(".hint").addClass('hint--showed');$(".js-close-hint").on("click",()=>{$(".hint").css('display','none');});});})();